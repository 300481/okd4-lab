; name servers - NS records
    IN      NS     ns.okd.

; name servers - A records
ns.okd.         IN      A      192.168.0.200
bootstrap.okd.  IN      A      192.168.0.151 ; Bootstrap
*.apps.okd.     IN      A      192.168.0.152 ; Master
api.okd.        IN      A      192.168.0.152 ; Master
api.okd.        IN      A      192.168.0.151 ; Bootstrap
api-int.okd.    IN      A      192.168.0.152 ; Master
api-int.okd.    IN      A      192.168.0.151 ; Bootstrap
master.okd.     IN      A      192.168.0.152 ; Master
